{
    "name": "joydeep-bhowmik/joy-ui",
    "description": "A Laravel/Alpine UI libray",
    "type": "library",
    "version": "v1.0.0",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel ui libary",
        "alpine ui libary"
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "JoydeepBhowmik\\JoyUI\\": "src/",
            "JoydeepBhowmik\\JoyUI\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "joydeep-bhowmik",
            "email": "bhowmikj9432@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "JoydeepBhowmik\\JoyUI\\Providers\\JoyUiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
